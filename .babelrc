{
  "presets": [[
      "@babel/preset-env",
      {
        "useBuiltIns": "usage", 
        "corejs": { 
          "version": 3, 
          "proposals": true 
        },
        "modules": false
      }
    ], "@babel/preset-react"],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "pure": true,
          "displayName": true,
          "ssr": false,
          "fileName": true
        }
      ],
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties"
  ]
}